{
  "name": "persons-service",
  "version": "0.0.7",
  "description": "Persons Service",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "start": "nodemon ./dist/index.js",
    "install:prod": "npm install --production",
    "lint": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern ./src/**/*",
    "lint:fix": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern ./src/**/* --fix",
    "test": "tsc && jest --maxWorkers=1 --testPathPattern .spec.ts$",
    "test:unit": "tsc && jest --testPathPattern .manager.spec.ts$ .repository.spec.ts$ .utils.spec.ts$",
    "test:integration": "tsc && jest --maxWorkers=1 --testPathPattern .router.spec.ts$",
    "coverage": "tsc && jest --coverage",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/yesodot/selenium/blueomek/persons-service.git"
  },
  "author": "Daniel Ventura",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "dotenv": "^16.0.0",
    "env-var": "^7.1.1",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "http": "0.0.1-security",
    "jayson": "^3.6.6",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/helmet": "0.0.48",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.17",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
